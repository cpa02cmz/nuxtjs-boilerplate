{
  "buildCommand": "npm run build",
  "outputDirectory": ".output",
  "installCommand": "npm ci --cache /tmp/.npm --prefer-offline",
  "framework": "nuxtjs",
  "regions": ["iad1"],
  "build": {
    "env": {
      "NITRO_PRESET": "vercel"
    }
  },
  "crons": [],
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/.vercel/functions/default/$1"
    }
  ]
}
